<% provide(:title, 'Sign up') %>

<h1>登録</h1>

<div class="container">
    <div class=" card">
        <div class="card-header bg-primary  text-white">
            ログインフォーム
        </div>
        
        <br>
        
<%= form_with model: @user do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :email %>
  <%= f.email_field :email %><br>
  <%= f.submit %>
<% end %>

